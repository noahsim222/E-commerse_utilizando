{
    "version":2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.js"
        },
        {
            "src":"/user",
            "dest":"/indexjs"
        },
         {
            "src":"/auth",
            "dest":"/index.js"
        },
         {
            "src":"/product",
            "dest":"/index.js"
        },
         {
            "src":"/email",
            "dest":"/index.js"
        },
         {
            "src":"/allUser",
            "dest":"/index.js"
        },
         {
            "src":"/messages",
            "dest":"/index.js"
        },
         {
            "src":"/all/messages",
            "dest":"/index.js"
        }

    ],
    "headers": [
        {
            "source": "/.*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://jordanb-xi.vercel.app"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,POST,PUT,DELETE,OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "X-Requested-With, Access-Control-Allow-Origin, X-HTTP-Method-Override, Content-Type, Authorization, Accept"
                }
            ]
        }
    ]
}
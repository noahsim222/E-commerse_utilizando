{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.js"
        },
        {
            "src": "/user",
            "dest": "/index.js"
        },
        {
            "src": "/auth",
            "dest": "/index.js"
        },
        {
            "src": "/product",
            "dest": "/index.js"
        },
        {
            "src": "/email",
            "dest": "/index.js"
        },
        {
            "src": "/allUser",
            "dest": "/index.js"
        },
        {
            "src": "/messages",
            "dest": "/index.js"
        },
        {
            "src": "/all/messages",
            "dest": "/index.js"
        }
    ]
}